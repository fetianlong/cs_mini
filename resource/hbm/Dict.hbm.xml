<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="com.dearho.cs.sys.pojo.Dict" table="s_dict">
		<id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="uuid.hex" />
        </id>
         <property name="name" type="java.lang.String">
        	<column name="name"  />
        </property>
        
        <property name="code" type="java.lang.String">
        	<column name="code"  />
        </property>
        
        <property name="cnName" type="java.lang.String">
        	<column name="cn_name"  />
        </property>
        
        <property name="groupId" type="java.lang.String">
        	<column name="group_id"  />
        </property>
        
        <property name="remark" type="java.lang.String">
        	<column name="remark"  />
        </property>
        
        <property name="isUsed" type="java.lang.Integer">
        	<column name="is_used"  />
        </property>
        <property name="isSystem" type="java.lang.Integer">
        	<column name="is_system"  />
        </property>
        <property name="color" type="string">
        	<column name="color"  />
        </property>
         
        <property name="createTime" type="java.util.Date">
        	<column name="create_time" />
        </property>
         <property name="creatorId" type="java.lang.String">
        	<column name="creator_id" length="32" />
        </property>
        
        <property name="updateTime" type="java.util.Date">
        	<column name="update_time" />
        </property>
        <property name="ts" type="java.util.Date">
        	<column name="ts" />
        </property>
	</class>
</hibernate-mapping>