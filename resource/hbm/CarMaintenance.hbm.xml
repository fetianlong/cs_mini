<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="com.dearho.cs.carservice.pojo.CarMaintenance" table="c_vehicle_maintenance">
		<id name="id" type="string">
            <column name="id" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="code" type="string">
        	<column name="code"  />
        </property>
        <property name="city" type="string">
        	<column name="city"  />
        </property>
        <property name="carId" type="string">
        	<column name="car_id"  />
        </property>
        
        <property name="vehicleModel" type="string">
        	<column name="vehicle_model"  />
        </property>
        <property name="Type" type="string">
        	<column name="type"  />
        </property>
        <property name="maintenanceTime" type="java.util.Date">
        	<column name="maintenance_time"  />
        </property>
       <property name="expectKm" type="double">
        	<column name="expect_km"  />
        </property>
         <property name="actualKm" type="double">
        	<column name="actual_km"  />
        </property>
         <property name="currentKm" type="double">
        	<column name="current_km"  />
        </property>
        <property name="feeType" type="string">
        	<column name="fee_type"  />
        </property>
        <property name="fee" type="double">
        	<column name="fee"  />
        </property>
        <property name="status" type="string">
        	<column name="status"  />
        </property>
        
        <property name="statusChangePerson1" type="string">
        	<column name="status_change_person1"  />
        </property>
        <property name="statusChangeTime1" type="java.util.Date">
        	<column name="status_change_time1"  />
        </property>
        <property name="statusChangePerson2" type="string">
        	<column name="status_change_person2"  />
        </property>
        <property name="statusChangeTime2" type="java.util.Date">
        	<column name="status_change_time2"  />
        </property>
        <property name="statusChangePerson3" type="string">
        	<column name="status_change_person3"  />
        </property>
        <property name="statusChangeTime3" type="java.util.Date">
        	<column name="status_change_time3"  />
        </property>
        
        <property name="remark" type="string">
        	<column name="remark"  />
        </property>
        <property name="isDiscard" type="integer">
        	<column name="is_discard"  />
        </property>
        <property name="creatorId" type="string">
        	<column name="creator_id" length="32" />
        </property>
        <property name="createTime" type="java.util.Date">
        	<column name="create_time" />
        </property>
        <property name="updateTime" type="java.util.Date">
        	<column name="update_time" />
        </property>
        <property name="ts" type="java.util.Date">
        	<column name="ts" />
        </property>
        
        <property name="maintenancePlace" type="string">
        	<column name="maintenance_Place" />
        </property>
        <property name="operator" type="string">
        	<column name="operator" />
        </property>
        
	</class>
</hibernate-mapping>