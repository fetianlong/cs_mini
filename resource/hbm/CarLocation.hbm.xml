<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dearho.cs.resmonitor.pojo.CarLocation" table="d_car_location">
		<id name="id" type="integer">
            <column name="ID" />
            <generator class="increment" /> 
        </id>
        <property name="deviceNo" type="java.lang.String">
            <column name="device_no"  />
        </property>
        <property name="lat" type="java.lang.Double">
            <column name="lat"  />
        </property>
        <property name="lng" type="java.lang.Double">
            <column name="lng"  />
        </property>
        <property name="speed" type="java.lang.Double">
            <column name="speed"  />
        </property>
        <property name="ts" type="java.util.Date">
            <column name="ts"  />
        </property>
	</class>
</hibernate-mapping>
