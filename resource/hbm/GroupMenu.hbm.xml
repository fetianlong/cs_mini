<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dearho.cs.sys.pojo.GroupMenu" table="s_group_menu">
		<id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="groupId" type="java.lang.String">
            <column name="group_id" length="32"  />
        </property>
        <property name="menuId" type="java.lang.String">
            <column name="menu_id" length="32"  />
        </property>
        <property name="creatorId" type="java.lang.String">
            <column name="creator_id" length="32" />
        </property>
     
        <property name="ts" type="java.util.Date">
       		<column name="ts"/>
       	</property>
	</class>
</hibernate-mapping>
