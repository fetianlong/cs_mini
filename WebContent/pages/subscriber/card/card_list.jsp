<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@  taglib prefix="ww" uri="webwork"%>
<%
String path = request.getContextPath();
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>

<title>Insert title here</title>
<%@ include file="/pages/common/common_head.jsp"%>

<script type="text/javascript">
	
	
	//添加车辆弹框
	function editEntity(id,type){
		var dialoguser = $.dialog({
		    id:'carList', 
		    title:(id == "")?"新添会员卡":"编辑会员卡",
			content : 'url:<%=path%>/subscriber/showCardDetail.action?subscriberCard.cardId='+id+'&type='+type,
			fixed:true,
			width:500,
			height:300,
			resize:false,
	 		max: false,
		    min: false,
		    lock: true,
		    ok: function(){
		    	var valid = this.content.isValid();
		    	if (valid){
		    		var form = this.content.getForm();
		    		showLoading(parent);
		    		$.post(form.attr("action"),form.serialize(),r_savedata,'json').error(requestError);
		    	}
		    	return false;
		    },
		    okVal:isnull(id)?'添加':'保存',
		    cancelVal: '关闭',
		    cancel: true,
		    close: function () {
		        this.hide();
		        restoreInfo('hospitalinfo');
		        return true;
		    },
		    init: function(){
		    	if (typeof this.content.isError != 'undefined'){
		    		$(":button").slice(0,1).hide();
		    	}
		    }
		});
	}
	
	function deleteEntity(){
		var ob = document.getElementsByName("checkdel");
		var check = false;
		for (var i = 0; i < ob.length; i++) {
			if (ob[i].checked) {
				check = true;
				break;
			}
		}
		if (!check) {
			alertinfo("请选择要删除的数据！");
			return false;
		}
		alertconfirm("确认删除选中的数据吗？",function (){
			showLoading();
			$.post('<%=path%>/subscriber/cardDelete.action',$('#sform').serialize(),r_delete,'json').error(requestError);
		});	
	}
	function r_savedata(data){
		hideLoading();
		switch(data.result){
			case 0:
				alertok(data.msg, function(){
			    	$('#sform').submit();		
			    });
				break;
			case 1:
				restoreInfo();
				alerterror(data.msg);
				break;
			case 9:
				document.location = "doError.action";
				break;
		}
	}
	function r_delete(data){
		hideLoading();
		switch(data.result){
			case 0:
				alertok("删除成功！", function(){
			    	$('#sform').submit();		
			    });
				break;
			case 1:
				restoreInfo();
				alerterror(data.msg);
				break;
			case 9:
				document.location = "doError.action";
				break;
		}
	}
	function deleteOneEntity(id){
		var pars={
				"id":id
			};
		alertconfirm("确认删除选中的数据吗？",function (){
			showLoading();
			$.post('carDelete.action',pars,r_delete,'json').error(requestError);
		});	
	}
	function r_save(data){
		hideLoading();
		switch(data.result){
			case 0:
				alertok("添加成功！");
				break;
			case 1:
				restoreInfo();
				alerterror(data.msg);
				break;
			case 9:
				document.location = "doError.action";
				break;
		}
	}
	function searchEntity(){
		$("#sform").submit();
	}


	function unbindingEntity(id){
		var dialoguser = $.dialog({
		    id:'carList', 
		    title:"解绑会员卡",
			content : 'url:<%=path%>/subscriber/showCardLoss.action?subscriberCardBinding.cardId='+id,
			fixed:true,
			width:500,
			height:300,
			resize:false,
	 		max: false,
		    min: false,
		    lock: true,
		    ok: function(){
		    	var valid = this.content.isValid();
		    	if (valid){
		    		var form = this.content.getForm();
		    		showLoading(parent);
		    		$.post(form.attr("action"),form.serialize(),r_savedata,'json').error(requestError);
		    	}
		    	return false;
		    },
		    okVal:'保存',
		    cancelVal: '关闭',
		    cancel: true,
		    close: function () {
		        this.hide();
		        restoreInfo('hospitalinfo');
		        return true;
		    },
		    init: function(){
		    	if (typeof this.content.isError != 'undefined'){
		    		$(":button").slice(0,1).hide();
		    	}
		    }
		});
	}

</script>
</head>
<body>
			<form name="sform" style="margin-left: 10px" id="sform" method="post" action="<%=path%>/subscriber/showCardList.action">
				<input type="hidden" name="page.orderFlag" id="page.orderFlag"
						value="<ww:property value="page.orderFlag"/>">
				<input type="hidden" name="page.orderString" id="page.orderString"
						value="<ww:property value="page.orderString"/>">
						
				<div class="chazhao">
					<div class="nr fl"><span>RFID：</span>
						<input type="text" class="kd" name="subscriberCard.cardId" value='<ww:property value="subscriberCard.cardId" />' />
					</div>
					<div class="nr fl">
					 	<span>状态：</span>
					 	<select class="kd"   name="subscriberCard.state" id="subscriberCard.state">
							<option>全部</option>
							
							<option value='<ww:property value="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_NORMAL"/>'   <ww:if test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_NORMAL.toString().equals(subscriberCard.state.toString())">selected</ww:if>>未绑定</option>
							<option value='<ww:property value="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_BIND"/>'   <ww:if test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_BIND.toString().equals(subscriberCard.state.toString())">selected</ww:if>>已绑定</option>
							<option  value='<ww:property value="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_UNBIND"/>'   <ww:if test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_UNBIND.toString().equals(subscriberCard.state.toString())">selected</ww:if>>已解定</option>
						</select>
					 </div>
					 
					 <div class="nr fl">
					 	<span>类型：</span>
					 	<select class="kd"   name="subscriberCard.type" id="subscriberCard">
							<option value="">全部</option>
							
							<ww:iterator value="@com.dearho.cs.sys.util.DictUtil@getDictsByGroupCode('15')" id="data" status="rl">
								<option value="<ww:property value="code" />"   <ww:if test="code.toString().equals(subscriberCard.type.toString())">selected</ww:if> ><ww:property value="cnName" /></option>	
							</ww:iterator>
						</select>
					 </div>
					
					<div class="nra"><a class="find_btn"  onclick="searchEntity();" target="_blank">查&nbsp;&nbsp;询</a></div>
					<div class="nra"><a class="add_btn" onclick="editEntity('','add');" target="_blank">添&nbsp;&nbsp;加</a></div>
  				    <div class="nra"><a class="del_btn" onclick="deleteEntity();" target="_blank">删&nbsp;&nbsp;除</a></div>
  				</div>
				<div style="width:100%; float:left;">
					<table class="biaoge" border="0"  cellspacing="0">
						<tr class="ths" id="tab_bg_cl">
							<td  width="68" height="50">
								<input type="checkbox" name="checkdelcheckall" onclick="funCheck('','checkdel')" />
							</td>
							<td width="100">
								<a href="javascript:SetOrder('cardId')">RFID<img src="<%=path%>/admin/common/images/main/paixu.png"/></a>
							</td>
							<td width="80">
								状态
							</td>
							<td width="150">
								厂商
							</td>
							<td width="150">
								<a href="javascript:SetOrder('createTime')">创建时间<img src="<%=path%>/admin/common/images/main/paixu.png"/></a>
							</td>
							<td width="100">
								绑定会员手机
							</td>
							<td width="80">
								绑定会员名称
							</td>
							
							
							<td width="157">
								操作
							</td>
						</tr>
						<ww:iterator value="page.results" id="data"  status="rl">
							<tr
								 <ww:if test="#rl.even"> class="trs"</ww:if> style="font-size:12px;">
								<td align="center">
									<input type="checkbox" name="checkdel" value="<ww:property value='cardId' />" />
								</td>
								<td align="left">
									<ww:property value="cardId" />
								</td>
								
								<td align="center">
									<ww:if test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_NORMAL.toString().equals(state.toString())">未绑定</ww:if>
									<ww:elseif test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_BIND.toString().equals(state.toString())">已绑定</ww:elseif>
									<ww:elseif test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_UNBIND.toString().equals(state.toString())">已解绑</ww:elseif>
									<ww:else>未知</ww:else>
								</td>
								<td align="left">
									<ww:property value="@com.dearho.cs.sys.util.DictUtil@getCnNameByCode('15',type)" />
								</td>
								<td align="center">
									<ww:property  value="transDateString(createTime)"/>	
								</td>
								<td align="center">
									<ww:property value="subscriberCardBinding.subscriber.phoneNo" />
								</td>
								<td align="left">
									<ww:property value="subscriberCardBinding.subscriber.name" />
								</td>
								
								<td>
									<div class="pan_btn3"  onclick="javascript:editEntity('<ww:property value="cardId"/>','update');">编辑</div>
									<ww:if test="@com.dearho.cs.subscriber.pojo.SubscriberCard@STATE_BIND.toString().equals(state.toString())">
										<div class="pan_btn4"  onclick="javascript:unbindingEntity('<ww:property value="cardId"/>');">解绑</div>
									</ww:if>
									
								</td>
							</tr>
						</ww:iterator>
						<tr style="background-color: #fff;height: 30px;">
							<td align="center" colspan="8">
								<ww:property value="page.pageSplit" />	
							</td>
						</tr>
					</table>
				</div>
